REM =======================================
Rem GANDOLF TROLL
REM Opens IE and plays a video of Gandalf
REM Created by the one and only A.K.
REM =======================================

DELAY 1000
GUI r
DELAY 1000
STRING powershell
ENTER
DELAY 200
STRING $WScript = New-Object -com wscript.shell; 1..50 | % { $WScript.SendKeys([char]175) }; 
STRING Start-Process iexplore -ArgumentList "-k https://player.vimeo.com/video/198392879?autoplay=1"
ENTER
STRING exit
ENTER
